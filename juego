num1 = int(input("introdusca un numero:  ") )
num2 = int(input("introdusca un segundo numero:  ") )
num3 = int(input("introdusca un terser numero:  ") )

if num1>num2 and num1>num3:
    print(f"el numero {num1} es el mayor")
elif num2>num1 and num2>num3:
    print(f"el numero {num2} es el mayor")
elif num3>num1 and num3>num2:
    print(f"el numero {num3} es el mayor")
